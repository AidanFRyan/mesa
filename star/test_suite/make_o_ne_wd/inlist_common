! convection
      mixing_length_alpha = 2
      use_Ledoux_criterion = .true.
      alpha_semiconvection = 0.01
      thermohaline_coeff = 2

      okay_to_reduce_gradT_excess = .true.
      gradT_excess_lambda1 = -1 ! turn on full all the time
      
! timesteps      
      varcontrol_target = 1d-3
      dX_nuc_drop_limit = 1d-2
      delta_HR_limit = 0.05

! Atm
      atm_option = 'T_tau'
      atm_T_tau_relation = 'Eddington'
      atm_T_tau_opacity = 'fixed'

! Overshoot
       overshoot_scheme(1) = 'exponential'
       overshoot_zone_type(1) = 'burn_Z'
       overshoot_zone_loc(1) = 'shell'
       overshoot_bdy_loc(1) = 'bottom'
       overshoot_f(1) = 0.014
       overshoot_f0(1) = 0.004

! Output
      num_trace_history_values = 2
      trace_history_value_name(1) = 'rel_E_err'
      trace_history_value_name(2) = 'log_rel_run_E_err'

      photo_interval = 50
      profile_interval = 100
      history_interval = 10
      terminal_interval = 10
      write_header_frequency = 10


/ ! end of controls namelist



