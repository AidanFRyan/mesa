#!/bin/bash

function check_okay {
    if [ $? -ne 0 ]
    then
        exit 1
    fi
}

function error {
    echo
    more diff.txt
    echo
    echo "******* failed ******** diff final_check.mod" "$1"
    echo
}

function do_one {
    cd $1
    if [ -z $3 ]
    then
        echo "$1 skip"
    elif [ "$3" == skip ]
    then
        echo "$1 skip"
    else
        pwd
        cp checks.md5 /Users/bpaxton/mesa/star/test_suite/$1/checks.md5
    fi
    cd ..
}

date
source do1_test_source
echo ""
date
