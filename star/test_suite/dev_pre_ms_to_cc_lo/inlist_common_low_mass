&controls

! wind

! atmosphere

! rotation

! mlt
      use_other_alpha_mlt = .true. ! implemented in run_star_extras
      x_ctrl(21) = 3.0 ! alpha_H
      x_ctrl(22) = 1.5 ! alpha_other
      x_ctrl(23) = 0.5 ! H_limit 
         ! use alpha_H if cell X >= H_limit; else use alpha_other
      x_ctrl(24) = 9d0 ! mass limit (Msun)
         ! use other_alpha_mlt only if star_mass >= this limit.

! mixing
      ! overshooting is turned on in inlist_to_end_core_he_burn, off otherwise.
      overshoot_scheme(1) = 'none'
      overshoot_zone_type(1) = 'any'
      overshoot_zone_loc(1) = 'any'
      overshoot_bdy_loc(1) = 'any'

! timesteps
      ! only for low mass until fix bug that is bad for high mass during period after core He burn until start core C burn
      ! once that bug is fixed, these should move to inlist_common_common
      min_timestep_factor = 0.8d0
      max_timestep_factor = 1.05d0
      timestep_factor_for_retries = 0.75

! mesh

! solver

! output

