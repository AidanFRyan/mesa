# run this from eosPT_builder to create eosPT_data.tar.xz in the eos directory
rm -rf ../data/eosPT_data/cache
cd data
rm -rf eosPT_data.tar
rm -rf eosPT_data.tar.xz
echo 'tar -cvf eosPT_data.tar eosPT_data'
tar -cvf eosPT_data.tar eosPT_data
echo 'xz eosPT_data.tar'
xz -v eosPT_data.tar
cp eosPT_data.tar.xz ../..
